inicio = int(input("Digite o primeiro valor: "))
fim = int(input("Digite o último valor: "))
incremento = int(input("Digite o incremento: "))

# Garante que o incremento seja positivo
if incremento <= 0:
    print("Incremento inválido!")
else:
    if inicio < fim:
        # Contagem crescente
        for i in range(inicio, fim + 1, incremento):
            print(i, end=" ")
    else:
        # Contagem regressiva
        for i in range(inicio, fim - 1, -incremento):
            print(i, end=" ")

    print("\nAcabou!")
